{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react",
    "@emotion/babel-preset-css-prop"
  ],
  "env": {
    "production": {
      "plugins": ["emotion", "@babel/plugin-syntax-dynamic-import"]
    },
    "development": {
      "plugins": [
        [
          "emotion",
          {
            "sourceMap": true
          }
        ],
        "@babel/plugin-syntax-dynamic-import"
      ]
    }
  }
}
